local Unf={}function Unf:None(s,b,bp,r)for i=1,#s do b[r][i]=string.byte(s,i)end end function Unf:Sub(s,b,bp,r)for i=1,#s do local x=string.byte(s,i)local a=(i>bp)and b[r][i-bp]or 0 b[r][i]=bit32.band(x+a,0xFF)end end function Unf:Up(s,b,bp,r)for i=1,#s do local x=string.byte(s,i)local bb=(r>1)and b[r-1][i]or 0 b[r][i]=bit32.band(x+bb,0xFF)end end function Unf:Average(s,b,bp,r)for i=1,#s do local x=string.byte(s,i)local a=(i>bp)and b[r][i-bp]or 0 local bb=(r>1)and b[r-1][i]or 0 b[r][i]=bit32.band(x+math.floor((a+bb)/2),0xFF)end end function Unf:Paeth(s,b,bp,r)for i=1,#s do local x=string.byte(s,i)local a=(i>bp)and b[r][i-bp]or 0 local bb=(r>1)and b[r-1][i]or 0 local c=(i>bp and r>1)and b[r-1][i-bp]or 0 local p=a+bb-c local pa,pb,pc=math.abs(p-a),math.abs(p-bb),math.abs(p-c)local pr=(pa<=pb and pa<=pc)and a or(pb<=pc)and bb or c b[r][i]=bit32.band(x+pr,0xFF)end end
local BR={}BR.__index=BR function BR.new(d)return setmetatable({D=d,P=1,L=#d},BR)end function BR:ReadByte()if self.P>self.L then return 0 end local b=string.byte(self.D,self.P)self.P=self.P+1 return b end function BR:ReadBytes(c)local r=string.sub(self.D,self.P,self.P+c-1)self.P=self.P+c return r end function BR:ReadString(l)return self:ReadBytes(l)end function BR:ReadInt32()local b1,b2,b3,b4=self:ReadByte(),self:ReadByte(),self:ReadByte(),self:ReadByte()return bit32.lshift(b1,24)+bit32.lshift(b2,16)+bit32.lshift(b3,8)+b4 end
local P={}P.__index=P function P.new(d)local r=BR.new(d)if r:ReadString(8)~="\137PNG\r\n\26\n"then error("Not PNG")end local ch,zl,rd={},{},true while rd do local l=r:ReadInt32()local t=r:ReadString(4)if l>0 then local dt=r:ReadBytes(l)if t=="IDAT"then table.insert(zl,dt)elseif t=="IHDR"then ch.IHDR=dt end end r:ReadInt32()if t=="IEND"then rd=false end end local rs,idx={},0 Def:InflateZlib({Input=BR.new(table.concat(zl)),Output=function(b)idx=idx+1 rs[idx]=string.char(b)end})local hr=BR.new(ch.IHDR)local w,h=hr:ReadInt32(),hr:ReadInt32()local dep=hr:ReadByte()local ct=hr:ReadByte()local bpp=math.max(1,((ct==2 and 3)or(ct==6 and 4)or 1)*(dep/8))local fl={W=w,H=h,B=bpp,Bit={}}local bf=BR.new(table.concat(rs))for row=1,h do local f=bf:ReadByte()local sl=bf:ReadBytes(w*bpp)fl.Bit[row]={}if f==0 then Unf:None(sl,fl.Bit,bpp,row)elseif f==1 then Unf:Sub(sl,fl.Bit,bpp,row)elseif f==2 then Unf:Up(sl,fl.Bit,bpp,row)elseif f==3 then Unf:Average(sl,fl.Bit,bpp,row)elseif f==4 then Unf:Paeth(sl,fl.Bit,bpp,row)end end return setmetatable(fl,P)end function P:GetPixel(x,y)if not self.Bit[y]then return 0,0,0,0 end local i=(x-1)*self.B+1 local r=self.Bit[y][i]or 0 local g=self.Bit[y][i+1]or 0 local b=self.Bit[y][i+2]or 0 local a=255 if self.B==4 then a=self.Bit[y][i+3]or 255 end return r,g,b,a end Pngl=P end)
